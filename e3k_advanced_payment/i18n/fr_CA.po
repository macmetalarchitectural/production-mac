#. module: e3k_advanced_payment
#: model:mail.template,body_html:e3k_advanced_payment.email_template_payment_notification
msgid ""
"\n"
"<p> The purpose of this communication is to inform you that a transfer has been made to your account in the amount of: <strong>${\"%.2f\" | format(object.amount)}  ${object.currency_id.name}</strong>\n"
"<br/>\n"
"which pays the following invoices:\n"
"</p>\n"
"<br/>\n"
" <table style=\"width:100%;border-top:1px solid #000;text-align: left;\">\n"
"                <thead>\n"
"                    <tr>  \n"
"                        <th class=\"text-left\">Numéro</th>\n"
"                        <th class=\"text-left\">Date de facturation</th>\n"
"                        <th class=\"text-left\">Montant</th>\n"
"                        <th class=\"text-left\">Escompte</th>\n"
"                        <th class=\"text-left\">Net</th>\n"
"                    </tr>\n"
"                </thead>\n"
"    </table>  \n"
"    \n"
"% for line in object.payment_partial_ids:      \n"
"    <table style=\"width:100%;border-top:1px solid #000;text-align: left;\">\n"
"                <tbody>\n"
"\n"
"                    <tr>\n"
"                        <td>${line.invoice_id.ref or ''| safe}</td>\n"
"                        <td>${line.invoice_id.invoice_date | safe}</td>\n"
"                        <td class=\"text-right\">${\"%.2f\" | format(line.amount_total) | safe}  ${line.invoice_id.currency_id.name}</td>\n"
"                        <td class=\"text-right\">${\"%.2f\" | format(line.done_discount_amount_in_out_refund)| safe} ${object.currency_id.name}</td>\n"
"                        <td class=\"text-right\">${\"%.2f\" | format(line.partial_payment) | safe} ${object.currency_id.name}</td>\n"
"                    </tr>\n"
"\n"
"                 \n"
"                </tbody>\n"
"\n"
"    </table>\n"
"% endfor\n"
"\n"
msgstr ""

