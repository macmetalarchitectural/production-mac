<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_picking_mac" inherit_id="stock.report_picking">
        <xpath expr="//div[@name='div_sched_date']" position="after">
            <div class="col-auto" name="div_sched_date">
                <strong>Order Date:</strong>
                <p t-field="o.sale_id.date_order"/>
            </div>
        </xpath>
        <xpath expr="//div[@name='div_sched_date']" position="replace">
            <div class="col-auto" name="div_sched_date">
                <strong>Scheduled Date:</strong>
                <p t-field="o.scheduled_date" t-options='{"format": "yyyy-MM-dd"}'/>
            </div>
        </xpath>
    </template>
</odoo>