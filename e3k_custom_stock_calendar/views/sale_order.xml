<?xml version="1.0"?>
<odoo>


     <record id="e3k_view_quotation_tree_with_onboarding" model="ir.ui.view">
            <field name="name">e3k.sale.order.tree</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_quotation_tree_with_onboarding" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="delivery_route" optional="show"/>
                  </xpath>
            </field>
        </record>
</odoo>
