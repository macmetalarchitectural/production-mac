<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <record id="account_move_form_view_inherited_configuration" model="ir.ui.view">
        <field name="name">account.move.form.view.inherited.configuration</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='accounting_info_group']" position="inside">
                <field name="print_ordered_qty" invisible="1"/>
                <field name="print_delivered_qty" invisible="1"/>
            </xpath>

         </field>
     </record>
</odoo>
