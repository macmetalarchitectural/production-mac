<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record model="ir.ui.view" id="stock_move_origin_customer_tree">
    <field eval="True" name="active"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='picking_partner_id']" position="after">
          <field name="origin_customer_id"/>
        </xpath>
      </data>
    </field>
    <field ref="stock.view_move_tree" name="inherit_id"/>
    <field name="mode">extension</field>
    <field name="model">stock.move</field>
    <field name="name">stock.move.tree: origin customer</field>
    <field name="priority">99</field>
    <field name="type">tree</field>
  </record>
  <record model="ir.ui.view" id="stock_move_origin_customer_form">
    <field eval="True" name="active"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='purchase_line_id']" position="after">
          <field name="origin_customer_id"/>
        </xpath>
      </data>
    </field>
    <field ref="stock.view_move_form" name="inherit_id"/>
    <field name="mode">extension</field>
    <field name="model">stock.move</field>
    <field name="name">stock.move.tree: origin customer</field>
    <field name="type">form</field>
  </record>
</odoo>
